{
  "name": "my-favorite-gear",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "serve": "vite preview",
    "deploy:prod": "firebase deploy --project=prod --only hosting,firestore",
    "deploy:dev": "firebase deploy --project=dev",
    "dev-deploy": "run-s setup:dev:firebase setup:prod:rules build:dev deploy:dev",
    "prod-deploy": "run-s setup:prod:* build:prod deploy:prod",
    "test": "jest",
    "test:fs": "run-s setup:test:rules \"jest --config jest.config.firestore.js\"",
    "test-all": "run-s \"firebase emulators:start\" test test:fs stop-all",
    "stop-all": "lsof -t -i:4000 -i:4001 -i:4400 -i:4500 -i:5000 -i:8080 -i:9099 | xargs kill -9",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "setup:test:rules": "NODE_ENV=test ts-node --skip-project configs/setup-rules.ts",
    "setup:prod:rules": "NODE_ENV=production ts-node --skip-project configs/setup-rules.ts",
    "setup:dev:firebase": "NODE_ENV=development ts-node --skip-project configs/setup-firebase.ts",
    "setup:prod:firebase": "NODE_ENV=production ts-node --skip-project configs/setup-firebase.ts"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run fix:eslint",
      "npm run fix:prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.8.2",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.5.1",
    "@mui/material": "5.5.3",
    "add": "2.0.6",
    "axios": "^0.26.1",
    "firebase": "^9.6.10",
    "lodash.isempty": "4.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "react-share": "^4.4.0",
    "web-vitals": "^2.1.4",
    "yarn": "1.22.18"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^1.3.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.15",
    "@types/lodash.isempty": "4.4.6",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-share": "^4.0.0",
    "@types/sprintf-js": "^1.1.2",
    "@types/testing-library__jest-dom": "^5.14.0",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-react": "1.2.0",
    "babel-jest": "^27.0.6",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "firebase-admin": "^10.0.2",
    "husky": "^7.0.4",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^12.3.7",
    "msw": "^0.39.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "2.8.6",
    "vitest": "0.7.12"
  },
  "msw": {
    "workerDirectory": "dist"
  },
  "prettier": {
    "singleQuote": false
  }
}
