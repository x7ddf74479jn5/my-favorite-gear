{
  "name": "my-favorite-gear",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --config ./webpack.dev.js --mode development",
    "build:prod": "webpack --progress --config ./webpack.prod.js --mode production",
    "build:dev": "webpack --progress --config ./webpack.stag.js --mode production",
    "deploy:prod": "firebase deploy --project=prod --only hosting,firestore",
    "deploy:dev": "firebase deploy --project=dev",
    "dev-deploy": "run-s setup:dev:firebase setup:prod:rules build:dev deploy:dev",
    "prod-deploy": "run-s setup:prod:* build:prod deploy:prod",
    "test": "jest",
    "test:fs": "run-s setup:test:rules \"jest --config jest.config.firestore.js\"",
    "test-all": "run-s \"firebase emulators:start\" test test:fs stop-all",
    "stop-all": "lsof -t -i:4000 -i:4001 -i:4400 -i:4500 -i:5000 -i:8080 -i:9099 | xargs kill -9",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "setup:test:rules": "NODE_ENV=test ts-node --skip-project configs/setup-rules.ts",
    "setup:prod:rules": "NODE_ENV=production ts-node --skip-project configs/setup-rules.ts",
    "setup:dev:firebase": "NODE_ENV=development ts-node --skip-project configs/setup-firebase.ts",
    "setup:prod:firebase": "NODE_ENV=production ts-node --skip-project configs/setup-firebase.ts"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run fix:eslint",
      "npm run fix:prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "firebase": "^8.6.8",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-firebaseui": "^5.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-share": "^4.4.0",
    "sprintf-js": "^1.1.2",
    "web-vitals": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@firebase/rules-unit-testing": "^1.3.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-share": "^4.0.0",
    "@types/sprintf-js": "^1.1.2",
    "@types/testing-library__jest-dom": "^5.14.0",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.6",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "firebase-admin": "^9.10.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "lint-staged": "^11.0.0",
    "msw": "^0.30.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.0.3",
    "unused-webpack-plugin": "^2.4.0",
    "webpack": "^5.41.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "msw": {
    "workerDirectory": "dist"
  }
}
