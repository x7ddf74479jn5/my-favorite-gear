{
  "name": "my-favorite-gear",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --config ./webpack.dev.js --mode development",
    "build": "webpack --progress --config ./webpack.prod.js --mode production",
    "build:dev": "webpack --progress --config ./webpack.stag.js --mode development",
    "deploy": "firebase deploy --project=prod",
    "deploy:dev": "firebase deploy --project=dev --force",
    "dev-deploy": "run-s build:dev deploy:dev",
    "test": "jest",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run fix:eslint",
      "npm run fix:prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "firebase": "^8.6.8",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-firebaseui": "^5.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-share": "^4.4.0",
    "sprintf-js": "^1.1.2",
    "web-vitals": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@firebase/rules-unit-testing": "^1.3.9",
    "@storybook/addon-a11y": "^6.4.0-alpha.5",
    "@storybook/addon-actions": "^6.4.0-alpha.5",
    "@storybook/addon-essentials": "^6.4.0-alpha.5",
    "@storybook/addon-links": "^6.4.0-alpha.5",
    "@storybook/addon-storyshots": "^6.4.0-alpha.5",
    "@storybook/builder-webpack5": "^6.4.0-alpha.5",
    "@storybook/manager-webpack5": "^6.4.0-alpha.5",
    "@storybook/node-logger": "^6.4.0-alpha.5",
    "@storybook/react": "^6.4.0-alpha.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.5",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-share": "^4.0.0",
    "@types/sprintf-js": "^1.1.2",
    "@types/testing-library__jest-dom": "^5.14.0",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
